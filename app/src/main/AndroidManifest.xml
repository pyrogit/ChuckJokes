<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="hr.pyro.chuckjokes.presentation">

    <uses-permission android:name="android.permission.INTERNET"/>

    <application
        android:name="hr.pyro.chuckjokes.presentation.ChuckJokeApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="hr.pyro.chuckjokes.presentation.view.activity.FavoriteJokesActivity"/>
        <activity android:name="hr.pyro.chuckjokes.presentation.view.activity.JokeDetailsActivity"/>
        <activity android:name="hr.pyro.chuckjokes.presentation.view.activity.RandomJokeActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>
